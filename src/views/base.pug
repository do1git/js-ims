doctype html
html(lang="ko")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css")
        //-link(rel="stylesheet" href="https://unpkg.com/mvp.css")
        link(rel="stylesheet" href="/static/dynamics/css/styles.css")

        title G&M OMS




    body 
        include partials/nav

        main 
            if messages.success
                div.flash.flash__success
                    span <i class="fas fa-check-square"></i>
                    span=messages.success
            if messages.error
                div.flash.flash__error
                    span <i class="fas fa-exclamation-triangle"></i>
                    span=messages.error
            include partials/header
            block content
        
        script(src="/static/dynamics/js/main.js") 
